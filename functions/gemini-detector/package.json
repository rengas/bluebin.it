{
  "name": "gemini-detector",
  "version": "1.0.0",
  "description": "Google Cloud Function for Gemini recyclable item detection",
  "main": "index.js",
  "scripts": {
    "start": "npx functions-framework --target=analyzeImage --port=8080",
    "deploy": "gcloud functions deploy analyzeImage --runtime=nodejs20 --trigger-http --allow-unauthenticated --entry-point=analyzeImage --region=asia-southeast1",
    "test": "node test.js"
  },
  "dependencies": {
    "@google-cloud/functions-framework": "^3.3.0",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "@google-cloud/functions": "^3.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
